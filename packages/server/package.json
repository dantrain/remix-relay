{
  "name": "@remix-relay/server",
  "version": "2.0.0",
  "description": "Provides Relay integration with React Router (Framework)",
  "keywords": [
    "Remix",
    "Relay",
    "React",
    "React Router"
  ],
  "homepage": "https://github.com/dantrain/remix-relay#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dantrain/remix-relay.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dan Train",
    "url": "https://github.com/dantrain"
  },
  "type": "commonjs",
  "exports": {
    ".": {
      "module": "./dist/index.mjs",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@graphql-tools/executor": "^1.3.10",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@remix-relay/eslint-config": "workspace:*",
    "@remix-relay/typescript-config": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.3",
    "@types/relay-runtime": "^18.2.3",
    "eslint": "^8.57.0",
    "graphql": "17.0.0-alpha.2",
    "relay-runtime": "^18.2.0",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2"
  },
  "peerDependencies": {
    "graphql": "17.0.0-alpha.2",
    "relay-runtime": "^16.1.0"
  }
}
