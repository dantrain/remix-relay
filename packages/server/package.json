{
  "name": "@remix-relay/server",
  "description": "Provides Relay integration with Remix",
  "version": "0.1.0",
  "author": "Dan Train",
  "keywords": [
    "Remix",
    "Relay",
    "React"
  ],
  "homepage": "https://github.com/dantrain/remix-relay#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dantraim/remix-relay.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "module": "./dist/index.mjs",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@apollo/server": "^4.9.5",
    "@remix-run/node": "^2.4.1",
    "graphql": "17.0.0-alpha.2",
    "relay-runtime": "^16.1.0"
  },
  "dependencies": {
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.5",
    "@remix-relay/eslint-config": "workspace:*",
    "@remix-relay/typescript-config": "workspace:*",
    "@remix-run/node": "^2.5.1",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.5",
    "@types/relay-runtime": "^14.1.22",
    "eslint": "^8.56.0",
    "graphql": "17.0.0-alpha.2",
    "relay-runtime": "^16.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
