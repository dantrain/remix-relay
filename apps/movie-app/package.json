{
  "name": "movie-app",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "zx ./scripts/dev.mjs",
    "lint": "eslint . --max-warnings 0 --ignore-path ../../.gitignore",
    "migrate:drizzle": "zx ./scripts/migrate-drizzle.mjs",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:prod": "wrangler d1 migrations apply DB --remote",
    "seed:local": "tsx ./scripts/seed.ts",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc --noEmit",
    "typegen": "wrangler types",
    "write-graphql-schema": "tsx ./scripts/write-graphql-schema.ts"
  },
  "dependencies": {
    "@graphql-tools/executor": "^1.3.9",
    "@graphql-yoga/plugin-defer-stream": "^3.10.6",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-relay": "^4.3.0",
    "@react-router/cloudflare": "^7.0.2",
    "@remix-relay/react": "workspace:*",
    "@remix-relay/server": "workspace:*",
    "@remix-relay/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "drizzle-orm": "^0.38.2",
    "graphql": "17.0.0-alpha.2",
    "graphql-yoga": "^5.10.6",
    "isbot": "^5.1.17",
    "lodash-es": "^4.17.21",
    "meros": "^1.3.0",
    "miniflare": "^3.20241205.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-relay": "^18.2.0",
    "react-router": "^7.0.2",
    "relay-compiler": "^18.2.0",
    "relay-runtime": "^18.2.0",
    "remix-auth": "^4.0.0",
    "remix-auth-github": "3.0.1",
    "tailwind-merge": "^2.5.5",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-router/dev": "^7.0.2",
    "@remix-relay/eslint-config": "workspace:*",
    "@remix-relay/tailwind-config": "workspace:*",
    "@remix-relay/typescript-config": "workspace:*",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/react-relay": "^18.2.0",
    "@types/relay-runtime": "^18.2.1",
    "babel-plugin-react-compiler": "19.0.0-beta-201e55d-20241215",
    "better-sqlite3": "^11.7.0",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.57.0",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-cjs-interop": "^2.1.6",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.4",
