{
  "name": "movie-app",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "zx ./scripts/dev.mjs",
    "lint": "eslint . --max-warnings 0 --ignore-path ../../.gitignore",
    "migrate:drizzle": "zx ./scripts/migrate-drizzle.mjs",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:prod": "wrangler d1 migrations apply DB --remote",
    "seed:local": "tsx ./scripts/seed.ts",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc --noEmit",
    "typegen": "wrangler types",
    "write-graphql-schema": "tsx ./scripts/write-graphql-schema.ts"
  },
  "dependencies": {
    "@graphql-tools/executor": "^1.3.2",
    "@graphql-yoga/plugin-defer-stream": "^3.7.0",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-relay": "^4.3.0",
    "@remix-relay/react": "workspace:*",
    "@remix-relay/server": "workspace:*",
    "@remix-relay/ui": "workspace:*",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "class-variance-authority": "^0.7.0",
    "drizzle-orm": "^0.36.0",
    "graphql": "17.0.0-alpha.2",
    "graphql-yoga": "^5.7.0",
    "isbot": "^5.1.17",
    "lodash-es": "^4.17.21",
    "meros": "^1.3.0",
    "miniflare": "^3.20241022.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-relay": "^18.1.0",
    "relay-compiler": "^18.1.0",
    "relay-runtime": "^18.1.0",
    "remix-auth": "^3.7.0",
    "remix-auth-github": "2.0.0",
    "tailwind-merge": "^2.5.4",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-relay/eslint-config": "workspace:*",
    "@remix-relay/tailwind-config": "workspace:*",
    "@remix-relay/typescript-config": "workspace:*",
    "@remix-run/dev": "^2.13.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-relay": "^16.0.6",
    "@types/relay-runtime": "^18.1.1",
    "better-sqlite3": "^11.5.0",
    "drizzle-kit": "^0.27.0",
    "eslint": "^8.57.0",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "vite": "^5.4.10",
    "vite-plugin-cjs-interop": "^2.1.4",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.3",
    "wrangler": "^3.84.0",
    "zx": "^8.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
