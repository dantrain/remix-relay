{
  "name": "movie-app",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "zx ./scripts/dev.mjs",
    "lint": "eslint . --max-warnings 0 --ignore-path ../../.gitignore",
    "migrate:drizzle": "zx ./scripts/migrate-drizzle.mjs",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:prod": "wrangler d1 migrations apply DB --remote",
    "seed:local": "tsx ./scripts/seed.ts",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc -b --noEmit",
    "typegen": "wrangler types",
    "write-graphql-schema": "tsx ./scripts/write-graphql-schema.ts"
  },
  "dependencies": {
    "@graphql-tools/executor": "^1.4.9",
    "@graphql-yoga/plugin-defer-stream": "^3.16.0",
    "@graphql-yoga/plugin-persisted-operations": "^3.16.0",
    "@pothos/core": "^4.9.1",
    "@pothos/plugin-relay": "^4.6.2",
    "@react-router/cloudflare": "^7.9.2",
    "@remix-relay/react": "workspace:*",
    "@remix-relay/server": "workspace:*",
    "@remix-relay/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "drizzle-orm": "^0.44.5",
    "graphql": "17.0.0-alpha.2",
    "graphql-yoga": "^5.16.0",
    "isbot": "^5.1.31",
    "lodash-es": "^4.17.21",
    "meros": "^1.3.2",
    "miniflare": "^4.20250924.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-relay": "^20.1.1",
    "react-router": "^7.9.2",
    "relay-compiler": "^20.1.1",
    "relay-runtime": "^20.1.1",
    "remix-auth": "^4.2.0",
    "remix-auth-github": "3.0.2",
    "tailwind-merge": "^3.3.1",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250926.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-router/dev": "^7.9.2",
    "@remix-relay/eslint-config": "workspace:*",
    "@remix-relay/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/better-sqlite3": "^7.6.13",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.2",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-cjs-interop": "^2.3.0",
    "vite-plugin-relay": "^2.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.40.1",
    "zx": "^8.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
